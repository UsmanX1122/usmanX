<body style="background: #eceff1">
  
</style>

  <div id="navbar">
    <div id="navbar">
      <section id="navbarHome">
        <nav class="navbar navbar-expand-lg navbar-dark bg-dark fixed-top py-0">
          <a class="hide-on-med-and-down" id="logo-container" href="\" class="brand-logo">
            <img src="./images/DSC_badge_Color.svg" width="55px" height="60px" style="margin-right: 4vh"></a>
        <a class="navbar-brand" href="#"><span style="color:#198efc;font-size: 20px; font-weight: 70px">Developer
            Student Club</span></a> &nbsp;
          <button class="navbar-toggler" type="button" data-toggle="collapse" data-target="#navbarSupportedContent"
            aria-controls="navbarSupportedContent" aria-expanded="false" aria-label="Toggle navigation">
            <span class="navbar-toggler-icon"></span>
          </button>

          <div class="collapse navbar-collapse" data-toggle="collapse" data-target=".navbar-collapse"
            id="navbarSupportedContent">
            <ul class="navbar-nav mr-auto nav-pills">
              <li class="nav-item active">
                <a class="nav-link hvr-fade py-2 px-3" href="/" id="ishank">Home<span
                    class="sr-only">(current)</span></a>
              </li> &nbsp;
              <li class="nav-item active">
                <a class="nav-link hvr-fade py-2 px-3 " href="/#aboutUs">About Us<span
                    class="sr-only">(current)</span></a>
              </li> &nbsp;
              <li class="nav-item active">
                <a class="nav-link hvr-fade py-2 px-3 current " href="#">Events</a>
              </li> &nbsp;

              <!-- <li class="nav-item active">
                <a class="nav-link hvr-fade py-2 px-3 " href="/blogs">Blogs</a>
              </li> &nbsp;
 -->

              <li class="nav-item active">
                <a class="nav-link hvr-fade py-2 px-3 #" href="/team">Team</a>
              </li> &nbsp;
              <li class="nav-item active">
                <a class="nav-link hvr-fade py-2 px-3 " href="/#contactUs">Contact Us</a>
              </li>
          </div>
        </nav>
    </div>


    <main role="main">

      <!-- Main jumbotron for a primary marketing message or call to action -->
      <br>
      <div class="container-fluid container">
        <div class="row">
          
          <% events.forEach(function(event) { %>
          <div class="col-md-4">
            <div class="card mb-4 box-shadow h-100 shadow-lg p-3 mb-5 bg-white rounded">
              <div class="box"><img class="card-img-top img1 " src="<%= event.image.url %>" 
                  data-src="holder.js/100px225?theme=thumb&bg=55595c&fg=eceeef&text=Thumbnail" alt="Card image cap">
              </div>
              <div class="card-body">
                <h4>Name: <%= event.name %></h4>&nbsp;
                <h6>Date & Day: <%= event.date %></h6>&nbsp;
                <h6>Venue: <%= event.venue %></h6>&nbsp;
                <h6>Hosted By: <%= event.host %></h6>&nbsp;
                <p class="card-text"><%= event.description %></p>
              </div>

            </div>

          </div>
          <% }); %>
        </div>
      </div>
    </main>

    <footer class="container">
      <div class="footer-copyright text-center py-3">2019 Copyright: Made with <i class="fa fa-heart"></i> by
        DSC-<a href="http://www.msrit.edu/" style="color:#198efc">RIT</a>
      </div>
    </footer>
    </script>
    <script type="text/javascript" src="https://cdnjs.cloudflare.com/ajax/libs/jquery/3.3.1/jquery.min.js">
    </script>
